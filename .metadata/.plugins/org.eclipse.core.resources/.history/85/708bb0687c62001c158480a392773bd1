
public class TestaVariaveis {

	public static void main(String[] args) {
		System.out.println("Olá, novo teste");
		
		int idade1;	
		idade1 = 37;
		
		System.out.println("Minha idade é " + idade1);
		
		int idade2;
		int idade3;
		idade2 = 30 + 10;
		idade3 = 7 * 5 + 2;
		
		System.out.println(idade2);
		System.out.println(idade3);
	}
}
