//Gerente é um Funcionario, Gerente herda da classe Funcionario
public class Gerente extends Funcionario { //extends Funcionario é um comando para a casse Gerente herdar tudo o que tem na classe Funcionario

	private int senha;
	
	
	public Gerente() {
		
	}
	
	public boolean autentica(int senha) {
		if(this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}
	
	public double getBonificacao() {
		return this.salario;
	}
	
}
